{
  "name": "common",
  "version": "0.0.1",
  "description": " common js code",
  "main": "index.js",
  "repository": "git@github.com:nonlux/common",
  "author": "nonlux <nonluxi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "fix-lint": "eslint --fix ./",
    "mocha:all": "better-npm-run mocha:all",
    "mocha:features": "better-npm-run mocha:features",
    "mocha:spec": "better-npm-run mocha:spec",
    "webpack-dev": "webpack-dev-server --inline --hot"
  },
  "betterScripts": {
    "mocha:all": {
      "command": "mocha --reporter mocha-better-spec-reporter --require ./src/resolve.js --require babel-polyfill --compilers js:babel-register src/**/*.spec.js test/index.js",
      "env": {
        "NODE_ENV": "test",
        "BABEL_CACHE_PATH": "./.cache/babel.json",
        "MOCHA_REPORTER_STACK_EXCLUDE": "node_modules/**"
      }
    },
    "mocha:features": {
      "command": "mocha --reporter mocha-better-spec-reporter --require ./src/resolve.js --require babel-polyfill --compilers js:babel-register test/index.js",
      "env": {
        "NODE_ENV": "test",
        "BABEL_CACHE_PATH": "./.cache/babel.json",
        "MOCHA_REPORTER_STACK_EXCLUDE": "node_modules/**"
      }
    },
    "mocha:spec": {
      "command": "mocha --require ./src/resolve.js --require babel-polyfill --compilers js:babel-register src/**/*.spec.js",
      "env": {
        "NODE_ENV": "test",
        "BABEL_CACHE_PATH": "./.cache/babel.json"
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "better-npm-run": "^0.0.13",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype-errors": "^2.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-better-spec-reporter": "^3.1.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "sinon": "^1.17.6",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "wepack": "^0.0.1",
    "yadda": "^0.22.1"
  },
  "dependencies": {
    "app-module-path": "^2.1.0",
    "react": "^15.4.1"
  }
}
